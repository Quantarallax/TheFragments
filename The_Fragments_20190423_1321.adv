####################################################################
## Set the style up
####################################################################

themes {
   fragments_theme : theme {
      derive_header_from_room_id       = true
      exit_list_style                  = verbose
      text_decorations                 = [ ]
# Uppercase, clean, smooth
#font                              = embedded_emulogic


#    font                             = embedded_apple2

# U/L, FONT SIZE INCREASE??    
#      font                             = embedded_zx_spectrum_7

# U/L, clean, smooth, bit SMALL!
#font = embedded_f25_bank_printer_bold
font = embedded_f25_bank_printer

# Clean, modern
#      font                              = monospace

      virtual_screen                    = fragments_screen
      lister                            = fragments_lister
      wait_align                        = left
#      layout = H D O
      colors { 
         story_pen         = #aaa
#         response_pen      = #bbb
         paper             = #000
         #prompt_pen       = #eee 
         textbox_pen       = #eee
         #negative_pen     = #000
         link_pen          = #ddd
      }

   }
